{"ast":null,"code":"import { computed } from 'vue';\nimport basicInfoBlock from '../basicInfoBlock.vue';\nimport { fishingGrounds } from '../../../data/skillFishing';\nimport { useSkillStore } from '../../../stores/expPark';\nimport BasicExpBar from '../BasicExpBar.vue';\nexport default {\n  components: {\n    basicInfoBlock,\n    BasicExpBar\n  },\n  props: {\n    selectedFish: {\n      required: true,\n      default: ''\n    },\n    fishGround: {\n      required: true\n    }\n  },\n  setup(props) {\n    const SkillStore = useSkillStore();\n    const getMasteryExp = SkillStore.getMasteryExp;\n    const maxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel));\n    const lastMaxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel - 1));\n    return {\n      getMasteryExp,\n      maxExp,\n      lastMaxExp\n    };\n  },\n  data() {\n    return {\n      isActived: false\n    };\n  },\n  methods: {\n    updateFishName() {\n      this.isActived = true;\n      this.$emit('updateFishName', {\n        fishName: this.selectedFish.name\n      });\n    },\n    stopWorking() {\n      this.isActived = false;\n      this.$emit('stopWorking');\n    }\n  },\n  computed: {\n    theFish() {\n      return fishingGrounds[this.fishGround].fishInGround[this.selectedFish.name];\n    },\n    masteryBarPercentage() {\n      return (this.theFish.masteryExp - this.lastMaxExp) / (this.maxExp - this.lastMaxExp) * 100 + '%';\n    }\n  },\n  watch: {\n    selectedFish() {\n      this.isActived = false;\n      const averageTime = (this.theFish.maxTime + this.theFish.minTime) / 2;\n      this.theFish.addMasteryExp = Math.round(this.getMasteryExp(this.theFish.masteryLevel, 23, averageTime));\n      this.theFish.addMasteryPoolExp = Math.ceil(this.theFish.addMasteryExp / 4);\n    }\n  }\n};","map":{"version":3,"names":["computed","basicInfoBlock","fishingGrounds","useSkillStore","BasicExpBar","components","props","selectedFish","required","default","fishGround","setup","SkillStore","getMasteryExp","maxExp","getMaxExp","masteryLevel","lastMaxExp","data","isActived","methods","updateFishName","$emit","fishName","name","stopWorking","theFish","fishInGround","masteryBarPercentage","masteryExp","watch","averageTime","maxTime","minTime","addMasteryExp","Math","round","addMasteryPoolExp","ceil"],"sources":["C:\\Users\\10433\\Downloads\\vues\\vue-cli-01-a-new-vue-project\\src\\components\\layout\\skills\\fishingScreen.vue"],"sourcesContent":["<template>\r\n\r\n  <span v-if= \"selectedFish ===''\">这里将会显示这个鱼池里要钓的鱼的信息</span>\r\n  <div v-else class=\"screen\">\r\n    <img class=\"logo\" :src=\"selectedFish.icon\">\r\n    <span>{{ selectedFish.name }}</span> \r\n    <div class=\"expPart\">\r\n        <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/xp.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.exp}}\r\n                </template>\r\n        </basic-info-block>\r\n        <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/mastery_header.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.addMasteryExp}}\r\n                </template>\r\n        </basic-info-block>\r\n        <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/mastery_pool.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.addMasteryPoolExp}}\r\n                </template>\r\n        </basic-info-block>\r\n    </div>\r\n    <p style=\"font-size: 0.85rem;\">钓鱼时间：{{ selectedFish.minTime }} - {{ selectedFish.maxTime }}秒</p>\r\n    <div class=\"masteryPart\">\r\n        <div class=\"iconPart\">\r\n            <img class=\"masteryIcon\" src=\"../../../assets/main/mastery_header.svg\">\r\n            <div class=\"masteryLevel\">{{ selectedFish.masteryLevel }}</div>\r\n        </div>\r\n        <div class=\"processBarPart\">\r\n            <div class=\"textPart\">{{ selectedFish.masteryExp }}/{{maxExp}}</div>\r\n            <basic-exp-bar \r\n            :bar-width=\"'100%'\"\r\n            :bar-height=\"'0.3rem'\"\r\n            :initial-pos=\"masteryBarPercentage\">\r\n            </basic-exp-bar>\r\n        </div>\r\n    </div>\r\n    <button v-if=\"!isActived\" @click=\"updateFishName\">开始钓鱼</button>\r\n    <button v-else @click=\"stopWorking\">结束钓鱼</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue';\r\nimport basicInfoBlock from '../basicInfoBlock.vue';\r\n\r\nimport { fishingGrounds } from '../../../data/skillFishing';\r\nimport { useSkillStore } from '../../../stores/expPark';\r\nimport BasicExpBar from '../BasicExpBar.vue';\r\nexport default {\r\n    components:{basicInfoBlock, BasicExpBar},\r\n\r\n    props:{\r\n        selectedFish:{\r\n            required:true,\r\n            default:'',\r\n        },\r\n        fishGround:{\r\n            required:true,\r\n\r\n        }\r\n    },\r\n    setup(props) {\r\n        const SkillStore = useSkillStore();\r\n        const getMasteryExp = SkillStore.getMasteryExp;\r\n        const maxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel));\r\n        const lastMaxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel - 1));\r\n        return{\r\n            getMasteryExp,\r\n            maxExp,\r\n            lastMaxExp\r\n        }\r\n    },\r\n    data(){\r\n        return{\r\n            isActived:false,\r\n        };\r\n        \r\n    },\r\n    methods:{\r\n        updateFishName(){\r\n            this.isActived = true;\r\n            this.$emit('updateFishName', {fishName: this.selectedFish.name});\r\n        },\r\n        stopWorking(){\r\n            this.isActived = false;\r\n            this.$emit('stopWorking')\r\n        }\r\n    },\r\n    computed:{\r\n        theFish(){\r\n            return  fishingGrounds[this.fishGround].fishInGround[this.selectedFish.name]\r\n        },\r\n        masteryBarPercentage(){\r\n            return ((this.theFish.masteryExp - this.lastMaxExp) / (this.maxExp - this.lastMaxExp)) * 100 + '%';\r\n        }\r\n    },\r\n    watch:{\r\n        selectedFish(){\r\n            this.isActived = false;\r\n            const averageTime = (this.theFish.maxTime + this.theFish.minTime)/2;\r\n            this.theFish.addMasteryExp = Math.round(this.getMasteryExp(this.theFish.masteryLevel,23,averageTime));\r\n            this.theFish.addMasteryPoolExp = Math.ceil(this.theFish.addMasteryExp/4);\r\n        }\r\n\r\n    }\r\n\r\n    \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .screen{\r\n        position: relative;\r\n        display: flex;\r\n        width: 100%;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    .expPart{\r\n        display: flex;\r\n        flex-direction: row;\r\n        gap: 1rem;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        box-sizing: border-box;\r\n        padding: 0.5rem 1rem;\r\n        width: 50%;\r\n        \r\n    }\r\n    .masteryPart{\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n        width: 100%;\r\n        height: 2rem;\r\n        padding: 0.5rem 0;\r\n        box-sizing: border-box;\r\n    }\r\n    .iconPart{\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 100%;\r\n        width: 10%;\r\n    }\r\n    .masteryIcon{\r\n        height: 1rem;\r\n        width: 1rem;\r\n    }\r\n    .masteryLevel{\r\n        font-size: 1rem;\r\n        font-weight: 700;\r\n    }\r\n    .processBarPart{\r\n        display: flex;\r\n        flex-direction: column;\r\n        width: 90%;\r\n        height: 2rem;\r\n        gap: 0.4rem;\r\n        align-items: center;\r\n    }\r\n    .textPart{\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        font-size: 0.7rem;\r\n    }\r\n    .action{\r\n        height: 1.5rem;\r\n        width: 1.5rem;\r\n    }\r\n    .logo{\r\n        position: absolute;\r\n        left: 1rem;\r\n        top: 1rem;\r\n        height: 2rem;\r\n        width: 2rem;\r\n    }\r\n</style>"],"mappings":"AA+CA,SAASA,QAAO,QAAS,KAAK;AAC9B,OAAOC,cAAa,MAAO,uBAAuB;AAElD,SAASC,cAAa,QAAS,4BAA4B;AAC3D,SAASC,aAAY,QAAS,yBAAyB;AACvD,OAAOC,WAAU,MAAO,oBAAoB;AAC5C,eAAe;EACXC,UAAU,EAAC;IAACJ,cAAc;IAAEG;EAAW,CAAC;EAExCE,KAAK,EAAC;IACFC,YAAY,EAAC;MACTC,QAAQ,EAAC,IAAI;MACbC,OAAO,EAAC;IACZ,CAAC;IACDC,UAAU,EAAC;MACPF,QAAQ,EAAC;IAEb;EACJ,CAAC;EACDG,KAAKA,CAACL,KAAK,EAAE;IACT,MAAMM,UAAS,GAAIT,aAAa,CAAC,CAAC;IAClC,MAAMU,aAAY,GAAID,UAAU,CAACC,aAAa;IAC9C,MAAMC,MAAK,GAAId,QAAQ,CAAC,MAAMY,UAAU,CAACG,SAAS,CAACT,KAAK,CAACC,YAAY,CAACS,YAAY,CAAC,CAAC;IACpF,MAAMC,UAAS,GAAIjB,QAAQ,CAAC,MAAMY,UAAU,CAACG,SAAS,CAACT,KAAK,CAACC,YAAY,CAACS,YAAW,GAAI,CAAC,CAAC,CAAC;IAC5F,OAAM;MACFH,aAAa;MACbC,MAAM;MACNG;IACJ;EACJ,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,SAAS,EAAC;IACd,CAAC;EAEL,CAAC;EACDC,OAAO,EAAC;IACJC,cAAcA,CAAA,EAAE;MACZ,IAAI,CAACF,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACG,KAAK,CAAC,gBAAgB,EAAE;QAACC,QAAQ,EAAE,IAAI,CAAChB,YAAY,CAACiB;MAAI,CAAC,CAAC;IACpE,CAAC;IACDC,WAAWA,CAAA,EAAE;MACT,IAAI,CAACN,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACG,KAAK,CAAC,aAAa;IAC5B;EACJ,CAAC;EACDtB,QAAQ,EAAC;IACL0B,OAAOA,CAAA,EAAE;MACL,OAAQxB,cAAc,CAAC,IAAI,CAACQ,UAAU,CAAC,CAACiB,YAAY,CAAC,IAAI,CAACpB,YAAY,CAACiB,IAAI;IAC/E,CAAC;IACDI,oBAAoBA,CAAA,EAAE;MAClB,OAAQ,CAAC,IAAI,CAACF,OAAO,CAACG,UAAS,GAAI,IAAI,CAACZ,UAAU,KAAK,IAAI,CAACH,MAAK,GAAI,IAAI,CAACG,UAAU,CAAC,GAAI,GAAE,GAAI,GAAG;IACtG;EACJ,CAAC;EACDa,KAAK,EAAC;IACFvB,YAAYA,CAAA,EAAE;MACV,IAAI,CAACY,SAAQ,GAAI,KAAK;MACtB,MAAMY,WAAU,GAAI,CAAC,IAAI,CAACL,OAAO,CAACM,OAAM,GAAI,IAAI,CAACN,OAAO,CAACO,OAAO,IAAE,CAAC;MACnE,IAAI,CAACP,OAAO,CAACQ,aAAY,GAAIC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACvB,aAAa,CAAC,IAAI,CAACa,OAAO,CAACV,YAAY,EAAC,EAAE,EAACe,WAAW,CAAC,CAAC;MAC