rdPackage[0].maxTime,this.randomValue);\r\n    this.timer = setInterval(()=>{  \r\n      this.randomValue = Math.random();\r\n      const RandthisRound = this.cumutiveProbablity(this.probabilityPool,this.randomValue);\r\n      if(RandthisRound === 0){\r\n        this.addItem(rewardPackage[0].name, rewardPackage[0].count);\r\n      }else{\r\n\r\n      }\r\n    },timeThisRound)\r\n  }\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.fishingGrounds{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-evenly;\r\n    gap: 1.5rem;\r\n    width: 100%;\r\n    padding: 1.5rem;\r\n    box-sizing: border-box;\r\n}\r\n.helperSturcture{\r\n  display: flex;\r\n  width: 48%;\r\n}\r\n\r\n</style>"],"mappings":";;EACKA,KAAK,EAAC;AAAgB;;;uBAD3BC,mBAAA,CAAAC,SAAA,SACAC,mBAAA,CAOM,OAPNC,UAOM,I,kBANJH,mBAAA,CAKMC,SAAA,QAPRG,WAAA,CAEgDC,KAAA,CAAAC,cAAc,EAAxBC,MAAM;yBAA1CP,mBAAA,CAKM;MALDD,KAAK,EAAC,iBAAiB;MACzBS,GAAG,EAAED,MAAM,CAACE;QACfC,YAAA,CAEgBC,wBAAA;MAFA,gBAAc,EAAEJ;IAAM;MAJxCK,OAAA,EAAAC,QAAA,CAI0C,MAExC,KAAAC,MAAA,QAAAA,MAAA,OANFC,gBAAA,CAI0C,UAExC,E;MANFC,CAAA;;oCASAd,mBAAA,CAA0E;IAAjEe,OAAK,EAAAH,MAAA,QAAAA,MAAA,MAAAI,MAAA,IAAEC,QAAA,CAAAC,cAAc,CAACf,KAAA,CAAAgB,cAAc,EAAChB,KAAA,CAAAiB,YAAY;KAAG,MAAI,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}