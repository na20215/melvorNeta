{"ast":null,"code":"import FishingScreen from './fishingScreen.vue';\nimport FishingUnit from './fishingUnit.vue';\nexport default {\n  components: {\n    FishingScreen,\n    FishingUnit\n  },\n  props: {\n    fishingGround: {\n      type: Object,\n      Required: true\n    }\n  },\n  data() {\n    return {\n      isLocked: false,\n      //禁止工作中的渔场切换鱼\n      selectedFish: ''\n    };\n  },\n  methods: {\n    stopFishing() {\n      this.$emit('stopFishing');\n    },\n    undateSelectedFish(data) {\n      this.selectedFish = data;\n      if (this.isLocked) {\n        // console.log(\"stopfishing 在fishingBlock触发了！\")\n        this.stopFishing();\n        this.isLocked = false;\n      }\n    },\n    addFishingGroundName(data) {\n      this.isLocked = true;\n      // console.log('updateGroundName')\n      this.$emit('addGroundName', {\n        groundName: this.fishingGround.name,\n        ...data\n      });\n    }\n  },\n  computed: {\n    fishProbablilty() {\n      const str = this.fishingGround.catchProbabilities.fish + \"%\";\n      return str;\n    },\n    trashProbablilty() {\n      const str = this.fishingGround.catchProbabilities.trash + \"%\";\n      return str;\n    },\n    specialItemProbablilty() {\n      const str = this.fishingGround.catchProbabilities.specialItem + \"%\";\n      return str;\n    }\n  }\n};","map":{"version":3,"names":["FishingScreen","FishingUnit","components","props","fishingGround","type","Object","Required","data","isLocked","selectedFish","methods","stopFishing","$emit","undateSelectedFish","addFishingGroundName","groundName","name","computed","fishProbablilty","str","catchProbabilities","fish","trashProbablilty","trash","specialItemProbablilty","specialItem"],"sources":["C:\\Users\\10433\\Downloads\\vues\\vue-cli-01-a-new-vue-project\\src\\components\\layout\\skills\\fishingBlock.vue"],"sourcesContent":["<template>\r\n    <basic-block class=\"cuttingblock\"\r\n    :side-color=\"'rgb(146,208,241)'\"\r\n  >\r\n  \r\n  <div class=\"header\">\r\n            <span class=\"groundName\">{{fishingGround.name}}</span>\r\n            <span class=\"groundInfo\">\r\n                <span>渔获:{{ fishProbablilty }}</span>\r\n                <span>垃圾:{{ trashProbablilty }}</span>\r\n                <span>特殊物品:{{ specialItemProbablilty }}</span>\r\n            </span>\r\n        </div>\r\n    <div class=\"inner_content\">\r\n        <div class=\"body\">\r\n          <div class=\"fishes\">\r\n            <fishing-unit\r\n              v-for=\"fish in fishingGround.fishInGround\"\r\n              :key=\"fish.name\"\r\n              :fish-name=\"fish.name\"\r\n              @update-selected=\"undateSelectedFish\">\r\n              <img class=\"icon\" :src=\"fish.icon\">\r\n              <div class=\"fishInfo\">\r\n                <span :style=\"{fontSize:'0.9rem'}\">{{ fish.name }}</span>\r\n                <span>{{ fish.exp }}技能经验值 {{ fish.minTime }}秒 - {{ fish.maxTime }}秒</span>\r\n              </div>\r\n            </fishing-unit>\r\n          </div>\r\n          <div class=\"view_window\">\r\n            <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/xp.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.exp}}\r\n                </template>\r\n        </basic-info-block>\r\n              <fishing-screen\r\n                :selected-fish=\"fishingGround.fishInGround[selectedFish]\"\r\n                :fish-ground=\"fishingGround.name\"\r\n                @update-fish-name=\"addFishingGroundName\"\r\n                @stop-working=\"stopFishing\"\r\n              ></fishing-screen>\r\n          </div>\r\n        </div>\r\n    </div>\r\n  </basic-block>\r\n</template>\r\n\r\n<script>\r\nimport FishingScreen from './fishingScreen.vue';\r\nimport FishingUnit from './fishingUnit.vue';\r\nexport default {\r\n  components:{ FishingScreen, FishingUnit},\r\n  props:{\r\n    fishingGround:{\r\n      type:Object,\r\n      Required:true,\r\n    }\r\n  },\r\n  data(){\r\n    return{\r\n      isLocked:false, //禁止工作中的渔场切换鱼\r\n      selectedFish :'',\r\n    }\r\n  },\r\n  methods:{\r\n  stopFishing(){\r\n    this.$emit('stopFishing');\r\n  },\r\n  undateSelectedFish(data){\r\n    this.selectedFish = data;\r\n    if(this.isLocked){\r\n      // console.log(\"stopfishing 在fishingBlock触发了！\")\r\n      this.stopFishing();\r\n      this.isLocked = false;\r\n    }\r\n  },\r\n  addFishingGroundName(data){\r\n    this.isLocked = true;\r\n    // console.log('updateGroundName')\r\n    this.$emit('addGroundName',{groundName: this.fishingGround.name , ...data});\r\n  }\r\n},\r\ncomputed:{\r\n  fishProbablilty(){\r\n    const str = this.fishingGround.catchProbabilities.fish + \"%\";\r\n    return str;\r\n  },\r\n  trashProbablilty(){\r\n    const str = this.fishingGround.catchProbabilities.trash + \"%\";\r\n    return str;\r\n  },\r\n  specialItemProbablilty(){\r\n    const str = this.fishingGround.catchProbabilities.specialItem + \"%\";\r\n    return str;\r\n  },\r\n},\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.inner_content{\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 0%;\r\n    margin: 0%;\r\n    width:100%;\r\n    height: 100%;\r\n}\r\n.header{\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    background-color: rgb(67,77,91);\r\n    width: 100%;\r\n    height: 2rem;\r\n    box-sizing: border-box;\r\n    padding-left:2rem;\r\n    padding-right:2rem;\r\n    border-radius: 0.1rem;\r\n\r\n}\r\n.header > .groundName{\r\n    display: inline-flex;\r\n    flex-direction: row;\r\n    gap:0.75rem;\r\n    font-size: 0.85rem;\r\n\r\n    align-items: center;\r\n}\r\n.header > .groundInfo{\r\n    display: inline-flex;\r\n    flex-direction: row;\r\n    gap:0.75rem;\r\n    font-size: 0.75rem;\r\n    justify-items: center;\r\n    text-align: center;\r\n    align-items: center;\r\n}\r\n\r\n.inner_content > .body{\r\n  display: flex;\r\n  flex-direction: row;\r\n  width: 100%;\r\n  height: 100%;\r\n  gap: 0%;\r\n  margin: 0%;\r\n  padding: 0%;\r\n}\r\n\r\n.inner_content > .body > .view_window{\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: center;\r\n  width: 50%;\r\n  height: 18rem;\r\n  gap: 0%;\r\n  margin: 0%;\r\n  padding: 1rem;\r\n}\r\n\r\n.inner_content > .body > .fishes{\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 50%;\r\n  height: 100%;\r\n  gap: 0.2rem;\r\n  padding: 1rem;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.fishInfo{\r\n  display: flex;\r\n  flex-direction: column;\r\n  font-size: 0.7rem;\r\n}\r\n\r\n.icon{\r\n  height: 2rem;\r\n  width: 2rem;\r\n}\r\n</style>"],"mappings":"AAgDA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EACbC,UAAU,EAAC;IAAEF,aAAa;IAAEC;EAAW,CAAC;EACxCE,KAAK,EAAC;IACJC,aAAa,EAAC;MACZC,IAAI,EAACC,MAAM;MACXC,QAAQ,EAAC;IACX;EACF,CAAC;EACDC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,QAAQ,EAAC,KAAK;MAAE;MAChBC,YAAW,EAAG;IAChB;EACF,CAAC;EACDC,OAAO,EAAC;IACRC,WAAWA,CAAA,EAAE;MACX,IAAI,CAACC,KAAK,CAAC,aAAa,CAAC;IAC3B,CAAC;IACDC,kBAAkBA,CAACN,IAAI,EAAC;MACtB,IAAI,CAACE,YAAW,GAAIF,IAAI;MACxB,IAAG,IAAI,CAACC,QAAQ,EAAC;QACf;QACA,IAAI,CAACG,WAAW,CAAC,CAAC;QAClB,IAAI,CAACH,QAAO,GAAI,KAAK;MACvB;IACF,CAAC;IACDM,oBAAoBA,CAACP,IAAI,EAAC;MACxB,IAAI,CAACC,QAAO,GAAI,IAAI;MACpB;MACA,IAAI,CAACI,KAAK,CAAC,eAAe,EAAC;QAACG,UAAU,EAAE,IAAI,CAACZ,aAAa,CAACa,IAAG;QAAI,GAAGT;MAAI,CAAC,CAAC;IAC7E;EACF,CAAC;EACDU,QAAQ,EAAC;IACPC,eAAeA,CAAA,EAAE;MACf,MAAMC,GAAE,GAAI,IAAI,CAAChB,aAAa,CAACiB,kBAAkB,CAACC,IAAG,GAAI,GAAG;MAC5D,OAAOF,GAAG;IACZ,CAAC;IACDG,gBAAgBA,CAAA,EAAE;MAChB,MAAMH,GAAE,GAAI,IAAI,CAAChB,aAAa,CAACiB,kBAAkB,CAACG,KAAI,GAAI,GAAG;MAC7D,OAAOJ,GAAG;IACZ,CAAC;IACDK,sBAAsBA,CAAA,EAAE;MACtB,MAAML,GAAE,GAAI,IAAI,CAAChB,aAAa,CAACiB,kBAAkB,CAACK,WAAU,GAAI,GAAG;MACnE,OAAON,GAAG;IACZ;EACF;AAEA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}