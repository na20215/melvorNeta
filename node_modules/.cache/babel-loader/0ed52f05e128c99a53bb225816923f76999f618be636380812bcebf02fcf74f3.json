{"ast":null,"code":"import { fishingGrounds } from '../../../data/skillFishing,js';\nimport fishingBlock from './fishingBlock.vue';\nimport FishingScreen from './fishingScreen.vue';\nimport FishingUnit from './fishingUnit.vue';\nexport default {\n  components: {\n    fishingBlock,\n    FishingUnit,\n    FishingScreen\n  },\n  data() {\n    return {\n      selectedFish: '',\n      fishingGrounds: fishingGrounds\n    };\n  },\n  methods: {\n    getFishInGround(ground) {\n      const fishInfo = ground.fishInGround;\n      if (Array.isArray(fishInfo)) {\n        return fishInfo;\n      }\n      return [];\n    },\n    getFishNames(ground) {\n      const fishInfo = ground.fishInGround;\n      if (Array.isArray(fishInfo)) {\n        return fishInfo.map(item => item.name);\n      }\n      return [];\n    },\n    undateSelectedFish(data) {\n      this.selectedFish = data;\n      console.log(typeof this.selectedFish);\n      console.log(this.selectedFish);\n    },\n    updateGroundSeletedFish(ground) {\n      ground.selectedFish = this.selectedFish;\n      return ground.selectedFish;\n    }\n  }\n};","map":{"version":3,"names":["fishingGrounds","fishingBlock","FishingScreen","FishingUnit","components","data","selectedFish","methods","getFishInGround","ground","fishInfo","fishInGround","Array","isArray","getFishNames","map","item","name","undateSelectedFish","console","log","updateGroundSeletedFish"],"sources":["C:\\Users\\10433\\Downloads\\vues\\vue-cli-01-a-new-vue-project\\src\\components\\layout\\skills\\skillFishing.vue"],"sourcesContent":["<template>\r\n<div class=\"fishingGrounds\">\r\n  <fishing-block\r\n    v-for=\"ground in fishingGrounds\"\r\n    :key=\"ground.name\">\r\n    <div class=\"header\">\r\n            <span class=\"groundName\">{{ground.name}}</span>\r\n            <span class=\"groundInfo\">\r\n                <span>渔获:{{ ground.catchProbabilities.fish }}</span>\r\n                <span>垃圾:{{ ground.catchProbabilities.trash }}</span>\r\n                <span>特殊物品:{{ ground.catchProbabilities.specialItem }}</span>\r\n            </span>\r\n        </div>\r\n    <div class=\"inner_content\">\r\n        <div class=\"body\">\r\n          <div class=\"fishes\">\r\n            <fishing-unit\r\n              v-for=\"fish in ground.fishInGround\"\r\n              :key=\"fish.name\"\r\n              :fish-name=\"fish.name\"\r\n              @update-selected=\"undateSelectedFish\">\r\n              <img class=\"icon\" :src=\"fish.icon\">\r\n              <div class=\"fishInfo\">\r\n                <span :style=\"{fontSize:'0.9rem'}\">{{ fish.name }}</span>\r\n                <span>{{ fish.exp }}技能经验值 {{ fish.minTime }}秒 - {{ fish.maxTime }}秒</span>\r\n              </div>\r\n            </fishing-unit>\r\n          </div>\r\n          <div class=\"view_window\">\r\n              <fishing-screen\r\n                :fishes-in-ground=\"getFishInGround(ground)\"\r\n                :fishes-name=\"getFishNames(ground)\"\r\n                :selected-fish=\"updateGroundSeletedFish(ground)\"\r\n              ></fishing-screen>\r\n          </div>\r\n        </div>\r\n    </div>\r\n  </fishing-block>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { fishingGrounds } from '../../../data/skillFishing,js';\r\nimport fishingBlock from './fishingBlock.vue';\r\nimport FishingScreen from './fishingScreen.vue';\r\nimport FishingUnit from './fishingUnit.vue';\r\nexport default {\r\n  components: { fishingBlock, FishingUnit, FishingScreen },\r\n    data() {\r\n  return {\r\n    selectedFish :'',\r\n    fishingGrounds: fishingGrounds,\r\n  };\r\n},\r\nmethods:{\r\n  getFishInGround(ground){\r\n    const fishInfo = ground.fishInGround;\r\n    if(Array.isArray(fishInfo)){\r\n      return fishInfo\r\n    }\r\n    return [];\r\n    \r\n  },\r\n  getFishNames(ground){\r\n  const fishInfo = ground.fishInGround;\r\n    if(Array.isArray(fishInfo)){\r\n      return fishInfo.map(item => item.name);\r\n    }\r\n    return [];\r\n  },\r\n  undateSelectedFish(data){\r\n    this.selectedFish = data;\r\n    console.log(typeof(this.selectedFish));\r\n    console.log(this.selectedFish);\r\n  },\r\n  updateGroundSeletedFish(ground){\r\n     ground.selectedFish = this.selectedFish;\r\n     return ground.selectedFish;\r\n  }\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.fishingGrounds{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-evenly;\r\n    gap: 1.5rem;\r\n    width: 100%;\r\n    padding: 1.5rem;\r\n    box-sizing: border-box;\r\n}\r\n\r\n.inner_content{\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 0%;\r\n    margin: 0%;\r\n    width:100%;\r\n    height: 100%;\r\n}\r\n.header{\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    background-color: rgb(67,77,91);\r\n    width: 100%;\r\n    height: 2rem;\r\n    box-sizing: border-box;\r\n    padding-left:2rem;\r\n    padding-right:2rem;\r\n    border-radius: 0.1rem;\r\n\r\n}\r\n.header > .groundName{\r\n    display: inline-flex;\r\n    flex-direction: row;\r\n    gap:0.75rem;\r\n    font-size: 0.85rem;\r\n\r\n    align-items: center;\r\n}\r\n.header > .groundInfo{\r\n    display: inline-flex;\r\n    flex-direction: row;\r\n    gap:0.75rem;\r\n    font-size: 0.75rem;\r\n    justify-items: center;\r\n    text-align: center;\r\n    align-items: center;\r\n}\r\n\r\n.inner_content > .body{\r\n  display: flex;\r\n  flex-direction: row;\r\n  width: 100%;\r\n  height: 100%;\r\n  gap: 0%;\r\n  margin: 0%;\r\n  padding: 0%;\r\n}\r\n\r\n.inner_content > .body > .view_window{\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: center;\r\n  width: 50%;\r\n  height: 18rem;\r\n  gap: 0%;\r\n  margin: 0%;\r\n  padding: 1rem;\r\n}\r\n\r\n.inner_content > .body > .fishes{\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 50%;\r\n  height: 100%;\r\n  gap: 0.2rem;\r\n  padding: 1rem;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.fishInfo{\r\n  display: flex;\r\n  flex-direction: column;\r\n  font-size: 0.7rem;\r\n}\r\n\r\n.icon{\r\n  height: 2rem;\r\n  width: 2rem;\r\n}\r\n</style>"],"mappings":"AA0CA,SAASA,cAAa,QAAS,+BAA+B;AAC9D,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,aAAY,MAAO,qBAAqB;AAC/C,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EACbC,UAAU,EAAE;IAAEH,YAAY;IAAEE,WAAW;IAAED;EAAc,CAAC;EACtDG,IAAIA,CAAA,EAAG;IACT,OAAO;MACLC,YAAW,EAAG,EAAE;MAChBN,cAAc,EAAEA;IAClB,CAAC;EACH,CAAC;EACDO,OAAO,EAAC;IACNC,eAAeA,CAACC,MAAM,EAAC;MACrB,MAAMC,QAAO,GAAID,MAAM,CAACE,YAAY;MACpC,IAAGC,KAAK,CAACC,OAAO,CAACH,QAAQ,CAAC,EAAC;QACzB,OAAOA,QAAO;MAChB;MACA,OAAO,EAAE;IAEX,CAAC;IACDI,YAAYA,CAACL,MAAM,EAAC;MACpB,MAAMC,QAAO,GAAID,MAAM,CAACE,YAAY;MAClC,IAAGC,KAAK,CAACC,OAAO,CAACH,QAAQ,CAAC,EAAC;QACzB,OAAOA,QAAQ,CAACK,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAI,CAAC;MACxC;MACA,OAAO,EAAE;IACX,CAAC;IACDC,kBAAkBA,CAACb,IAAI,EAAC;MACtB,IAAI,CAACC,YAAW,GAAID,IAAI;MACxBc,OAAO,CAACC,GAAG,CAAC,OAAO,IAAI,CAACd,YAAa,CAAC;MACtCa,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,YAAY,CAAC;IAChC,CAAC;IACDe,uBAAuBA,CAACZ,MAAM,EAAC;MAC5BA,MAAM,CAACH,YAAW,GAAI,IAAI,CAACA,YAAY;MACvC,OAAOG,MAAM,CAACH,YAAY;IAC7B;EACF;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}