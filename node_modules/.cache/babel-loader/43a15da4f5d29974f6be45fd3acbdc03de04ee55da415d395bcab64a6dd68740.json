.isActived = false;\r\n            const averageTime = (selectedFish.maxTime + rewardPackage[0].minTime)/2;\r\n            rewardPackage[0].addMasteryExp = Math.round(this.getMasteryExp(rewardPackage[0].masteryLevel,23,averageTime));\r\n            rewardPackage[0].addMasteryPoolExp = Math.ceil(rewardPackage[0].addMasteryExp/4)\r\n        }\r\n\r\n    }\r\n\r\n    \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .screen{\r\n        position: relative;\r\n        display: flex;\r\n        width: 100%;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    .expPart{\r\n        display: flex;\r\n        flex-direction: column;\r\n        gap: 1rem;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        box-sizing: border-box;\r\n        padding: 0 1rem;\r\n        width: 100%;\r\n    }\r\n    .action{\r\n        height: 2rem;\r\n        width: 2rem;\r\n    }\r\n    img{\r\n        position: absolute;\r\n        left: 1rem;\r\n        top: 1rem;\r\n        height: 2rem;\r\n        width: 2rem;\r\n    }\r\n</style>"],"mappings":"AAiCA,OAAOA,cAAa,MAAO,uBAAuB;AAClD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,eAAe;EACXC,UAAU,EAAC;IAACD;EAAc,CAAC;EAC3BE,KAAK,EAAC;IACFC,YAAY,EAAC;MACTC,QAAQ,EAAC,IAAI;MACbC,OAAO,EAAC;IACZ;EACJ,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,SAAS,EAAC;IACd,CAAC;EAEL,CAAC;EACDC,OAAO,EAAC;IACJC,cAAcA,CAAA,EAAE;MACZ,IAAI,CAACF,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACG,KAAK,CAAC,gBAAgB,EAAE;QAACC,QAAQ,EAAE,IAAI,CAACR,YAAY,CAACS;MAAI,CAAC,CAAC;IACpE,CAAC;IACDC,WAAWA,CAAA,EAAE;MACT,IAAI,CAACN,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACG,KAAK,CAAC,aAAa;IAC5B;EACJ,CAAC;EACDI,KAAK,EAAC;IACFX,YAAYA,CAAA,EAAE;MACV,IAAI,CAACI,SAAQ,GAAI,KAAK;MACtB,MAAMQ,WAAU,GAAI,CAACZ,YAAY,CAACa,OAAM,GAAIC,aAAa,CAAC,CAAC,CAAC,CAACC,OAAO,IAAE,CAAC;MACvED,aAAa,CAAC,CAAC,CAAC,CAACE,aAAY,GAAIC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACC,aAAa,CAACL,aAAa,CAAC,CAAC,CAAC,CAACM,YAAY,EAAC,EAAE,EAACR,WAAW,CAAC,CAAC;MAC7GE,aA