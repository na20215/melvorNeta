{"ast":null,"code":"import { fishingGrounds } from '../../../data/skillFishing,js';\nimport fishingBlock from './fishingBlock.vue';\nexport default {\n  components: {\n    fishingBlock\n  },\n  data() {\n    return {\n      selectedFish: '',\n      fishingGrounds: fishingGrounds\n    };\n  },\n  methods: {\n    getFishInGround(ground) {\n      const fishInfo = ground.fishInGround;\n      if (Array.isArray(fishInfo)) {\n        return fishInfo;\n      }\n      return [];\n    },\n    getFishNames(ground) {\n      const fishInfo = ground.fishInGround;\n      if (Array.isArray(fishInfo)) {\n        return fishInfo.map(item => item.name);\n      }\n      return [];\n    },\n    undateSelectedFish(data) {\n      this.selectedFish = data;\n      console.log(typeof this.selectedFish);\n      console.log(this.selectedFish);\n    },\n    updateGroundSeletedFish(ground) {\n      ground.selectedFish = this.selectedFish;\n      return ground.selectedFish;\n    }\n  }\n};","map":{"version":3,"names":["fishingGrounds","fishingBlock","components","data","selectedFish","methods","getFishInGround","ground","fishInfo","fishInGround","Array","isArray","getFishNames","map","item","name","undateSelectedFish","console","log","updateGroundSeletedFish"],"sources":["C:\\Users\\10433\\Downloads\\vues\\vue-cli-01-a-new-vue-project\\src\\components\\layout\\skills\\skillFishing.vue"],"sourcesContent":["<template>\r\n<div class=\"fishingGrounds\">\r\n  <div class=\"helperSturcture\" v-for=\"ground in fishingGrounds\"\r\n    :key=\"ground.name\">\r\n  <fishing-block :fishing-ground=\"ground\">\r\n    \r\n  </fishing-block>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { fishingGrounds } from '../../../data/skillFishing,js';\r\nimport fishingBlock from './fishingBlock.vue';\r\n\r\nexport default {\r\n  components: { fishingBlock },\r\n    data() {\r\n  return {\r\n    selectedFish :'',\r\n    fishingGrounds: fishingGrounds,\r\n  };\r\n},\r\nmethods:{\r\n  getFishInGround(ground){\r\n    const fishInfo = ground.fishInGround;\r\n    if(Array.isArray(fishInfo)){\r\n      return fishInfo\r\n    }\r\n    return [];\r\n    \r\n  },\r\n  getFishNames(ground){\r\n  const fishInfo = ground.fishInGround;\r\n    if(Array.isArray(fishInfo)){\r\n      return fishInfo.map(item => item.name);\r\n    }\r\n    return [];\r\n  },\r\n  undateSelectedFish(data){\r\n    this.selectedFish = data;\r\n    console.log(typeof(this.selectedFish));\r\n    console.log(this.selectedFish);\r\n  },\r\n  updateGroundSeletedFish(ground){\r\n     ground.selectedFish = this.selectedFish;\r\n     return ground.selectedFish;\r\n  }\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.fishingGrounds{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-evenly;\r\n    gap: 1.5rem;\r\n    width: 100%;\r\n    padding: 1.5rem;\r\n    box-sizing: border-box;\r\n}\r\n.helperSturcture{\r\n  display: flex;\r\n  width: 48%;\r\n}\r\n\r\n</style>"],"mappings":"AAYA,SAASA,cAAa,QAAS,+BAA+B;AAC9D,OAAOC,YAAW,MAAO,oBAAoB;AAE7C,eAAe;EACbC,UAAU,EAAE;IAAED;EAAa,CAAC;EAC1BE,IAAIA,CAAA,EAAG;IACT,OAAO;MACLC,YAAW,EAAG,EAAE;MAChBJ,cAAc,EAAEA;IAClB,CAAC;EACH,CAAC;EACDK,OAAO,EAAC;IACNC,eAAeA,CAACC,MAAM,EAAC;MACrB,MAAMC,QAAO,GAAID,MAAM,CAACE,YAAY;MACpC,IAAGC,KAAK,CAACC,OAAO,CAACH,QAAQ,CAAC,EAAC;QACzB,OAAOA,QAAO;MAChB;MACA,OAAO,EAAE;IAEX,CAAC;IACDI,YAAYA,CAACL,MAAM,EAAC;MACpB,MAAMC,QAAO,GAAID,MAAM,CAACE,YAAY;MAClC,IAAGC,KAAK,CAACC,OAAO,CAACH,QAAQ,CAAC,EAAC;QACzB,OAAOA,QAAQ,CAACK,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAI,CAAC;MACxC;MACA,OAAO,EAAE;IACX,CAAC;IACDC,kBAAkBA,CAACb,IAAI,EAAC;MACtB,IAAI,CAACC,YAAW,GAAID,IAAI;MACxBc,OAAO,CAACC,GAAG,CAAC,OAAO,IAAI,CAACd,YAAa,CAAC;MACtCa,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,YAAY,CAAC;IAChC,CAAC;IACDe,uBAAuBA,CAACZ,MAAM,EAAC;MAC5BA,MAAM,CAACH,YAAW,GAAI,IAAI,CAACA,YAAY;MACvC,OAAOG,MAAM,CAACH,YAAY;IAC7B;EACF;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}