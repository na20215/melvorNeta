{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BasicInfoBlock from '../basicInfoBlock.vue';\nimport cuttingBlock from './cuttingBlock.vue';\nimport { useWarehouseStore } from '../../../stores/warehouse';\nimport { useActiveStore } from '../../../stores/skillPark';\nexport default {\n  components: {\n    cuttingBlock,\n    BasicInfoBlock\n  },\n  setup() {\n    const WarehouseStore = useWarehouseStore();\n    const activeStateHandler = useActiveStore();\n    const additem = WarehouseStore.addItem;\n    const activeSkill = activeStateHandler.activeSkill;\n    const activeData = activeStateHandler.activeData;\n    const skillData = activeStateHandler.skillData;\n    return {\n      additem,\n      activeSkill,\n      activeData,\n      skillData\n    };\n  },\n  data() {\n    return {\n      isActived: false,\n      DoubleCutting: false,\n      treesData: {\n        '普通树': {\n          name: '普通树',\n          icon: require('@/assets/woodcutting/normal_tree.svg'),\n          requirementLevel: 1,\n          exp: 10,\n          cutTime: 3,\n          isActive: false,\n          reward: '普通原木',\n          rewardIcon: require('@/assets/bank/logs_normal.png'),\n          timer: null\n        },\n        '橡树': {\n          name: '橡树',\n          icon: require('@/assets/woodcutting/oak_tree.svg'),\n          requirementLevel: 10,\n          exp: 15,\n          cutTime: 4,\n          isActive: false,\n          reward: '橡木',\n          rewardIcon: require('@/assets/bank/logs_oak.png'),\n          timer: null\n        },\n        '柳树': {\n          name: '柳树',\n          icon: require('@/assets/woodcutting/willow_tree.svg'),\n          requirementLevel: 25,\n          exp: 22,\n          cutTime: 5,\n          isActive: false,\n          reward: '柳木',\n          rewardIcon: require('@/assets/bank/logs_willow.png'),\n          timer: null\n        },\n        '柚树': {\n          name: '柚树',\n          icon: require('@/assets/woodcutting/teak_tree.svg'),\n          requirementLevel: 35,\n          exp: 30,\n          cutTime: 6,\n          isActive: false,\n          reward: '柚木',\n          rewardIcon: require('@/assets/bank/logs_teak.png'),\n          timer: null\n        },\n        '枫树': {\n          name: '枫树',\n          icon: require('@/assets/woodcutting/maple_tree.svg'),\n          requirementLevel: 45,\n          exp: 40,\n          cutTime: 8,\n          isActive: false,\n          reward: '枫木',\n          rewardIcon: require('@/assets/bank/logs_maple.png'),\n          timer: null\n        },\n        '桃花芯树': {\n          name: '桃花芯树',\n          icon: require('@/assets/woodcutting/mahogany_tree.svg'),\n          requirementLevel: 55,\n          exp: 60,\n          cutTime: 10,\n          isActive: false,\n          reward: '桃花芯木',\n          rewardIcon: require('@/assets/bank/logs_mahogany.png'),\n          timer: null\n        },\n        '紫衫': {\n          name: '紫衫',\n          icon: require('@/assets/woodcutting/yew_tree.svg'),\n          requirementLevel: 60,\n          exp: 80,\n          cutTime: 12,\n          isActive: false,\n          reward: '紫衫木',\n          rewardIcon: require('@/assets/bank/logs_yew.png'),\n          timer: null\n        },\n        '云杉': {\n          name: '云杉',\n          icon: require('@/assets/woodcutting/spruce_tree.svg'),\n          requirementLevel: 70,\n          exp: 100,\n          cutTime: 15,\n          isActive: false,\n          reward: '云杉原木',\n          rewardIcon: require('@/assets/bank/logs_spruce.png'),\n          timer: null\n        },\n        '菩提树': {\n          name: '菩提树',\n          icon: require('@/assets/woodcutting/linden_tree.svg'),\n          requirementLevel: 80,\n          exp: 140,\n          cutTime: 18,\n          isActive: false,\n          reward: '菩提原木',\n          rewardIcon: require('@/assets/bank/logs_linden.png'),\n          timer: null\n        },\n        '接骨木树': {\n          name: '接骨木树',\n          icon: require('@/assets/woodcutting/elderwood_tree.svg'),\n          requirementLevel: 85,\n          exp: 180,\n          cutTime: 20,\n          isActive: false,\n          reward: '接骨木',\n          rewardIcon: require('@/assets/bank/logs_elderwood.png'),\n          timer: null\n        },\n        '红木': {\n          name: '红木',\n          icon: require('@/assets/woodcutting/redwood_tree.svg'),\n          requirementLevel: 90,\n          exp: 195,\n          cutTime: 22,\n          isActive: false,\n          reward: '红木',\n          rewardIcon: require('@/assets/bank/logs_redwood.png'),\n          timer: null\n        },\n        '魔法树': {\n          name: '魔法树',\n          icon: require('@/assets/woodcutting/magic_tree.svg'),\n          requirementLevel: 95,\n          exp: 225,\n          cutTime: 25,\n          isActive: false,\n          reward: '魔法原木',\n          rewardIcon: require('@/assets/bank/logs_magic.png'),\n          timer: null\n        }\n      }\n    };\n  },\n  methods: {\n    switchActiveState(tree) {\n      if (this.activeData.length < 2 && tree.isActive === false) {\n        if (!this.isActived) {\n          this.activeSkill = '伐木';\n          this.skillData = Object.assign({}, this.treesData);\n          this.isActived = true;\n        }\n        tree.isActive = true;\n        this.activeData.push(tree);\n        console.log(this.activeData);\n        this.addTimer(tree);\n        return;\n      }\n      if (this.activeData.includes(tree) || tree.isActive === true) {\n        if (this.activeData.length === 0) {\n          this.activeSkill = '伐木';\n          this.isActived = false;\n        }\n        tree.isActive = false;\n        const index = this.activeData.indexOf(tree);\n        this.activeData.splice(index, 1);\n        this.removeTimer(tree);\n        return;\n      }\n    },\n    addTimer(tree) {\n      tree.timer = setInterval(() => {\n        this.additem(tree.reward, 1);\n      }, tree.cutTime * 1000);\n      console.log(tree.timer);\n    },\n    removeTimer(tree) {\n      clearInterval(tree.timer);\n    },\n    currentTreeDataSource() {\n      this.skillData = Object.assign({}, this.treesData);\n      return this.skillData;\n    }\n  },\n  computed: {\n    //     currentTreeDataSource() {\n    //     const dataSource = this.activeSkill === '伐木' ? this.skillData : this.treesData;\n    //     console.log('currentTreeDataSource:', dataSource);\n    //     return dataSource;\n    // },\n  },\n  watch() {\n    return {};\n  }\n};","map":{"version":3,"names":["BasicInfoBlock","cuttingBlock","useWarehouseStore","useActiveStore","components","setup","WarehouseStore","activeStateHandler","additem","addItem","activeSkill","activeData","skillData","data","isActived","DoubleCutting","treesData","name","icon","require","requirementLevel","exp","cutTime","isActive","reward","rewardIcon","timer","methods","switchActiveState","tree","length","Object","assign","push","console","log","addTimer","includes","index","indexOf","splice","removeTimer","setInterval","clearInterval","currentTreeDataSource","computed","watch"],"sources":["C:\\Users\\10433\\Downloads\\vues\\vue-cli-01-a-new-vue-project\\src\\components\\layout\\skills\\skillCutting.vue"],"sourcesContent":["<template>\r\n    <div>\r\n    <div class=\"cuttingAction\">\r\n        <basic-block class=\"header\"\r\n        :block-width=\"'100%'\"\r\n        :side-color=\"'rgb(53,143,18)'\">\r\n            <basic-exp-bar\r\n                :bar-height=\"'2rem'\"\r\n                :bar-width=\"'95%'\"\r\n                :initial-pos=\"'0%'\"\r\n            ></basic-exp-bar>\r\n            <span v-show=\"activeData.length === 0\">这里将会显示关于你的伐木动作的信息</span>\r\n            <div class=\"detail\" v-if=\"activeData.length !== 0\">\r\n                <div\r\n                v-for=\"(tree, idx) in activeData\"\r\n                :key=\"idx\"\r\n                class=\"outerEdge\">\r\n                    <div class=\"expPart\">\r\n                        <basic-info-block>\r\n                            <img class=\"action\" :src=\"tree.rewardIcon\">\r\n                            <template v-slot:count>\r\n                                1\r\n                            </template>\r\n                        </basic-info-block>\r\n                        <basic-info-block>\r\n                            <img class=\"action\" src=\"../../../assets/main/mastery_header.svg\">\r\n                            <template v-slot:count>\r\n                                {{tree.exp}}\r\n                            </template>\r\n                        </basic-info-block>\r\n                    </div>\r\n                    <div class=\"masteryPart\">\r\n                        <basic-info-block>\r\n                            <img class=\"action\" src=\"../../../assets/main/xp.svg\">\r\n                            <template v-slot:count>\r\n                                {{tree.exp}}\r\n                            </template>\r\n                        </basic-info-block>\r\n                        <basic-info-block>\r\n                            <img class=\"action\" src=\"../../../assets/main/mastery_pool.svg\">\r\n                            <template v-slot:count>\r\n                                {{tree.exp}}\r\n                            </template>\r\n                        </basic-info-block>\r\n                        <basic-info-block\r\n                            :banner-color=\"'rgb(81,121,214)'\">\r\n                            <img class=\"action\" src=\"../../../assets/main/timer.svg\">\r\n                            <template v-slot:count>\r\n                                {{tree.cutTime}} 秒\r\n                            </template>\r\n                        </basic-info-block>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </basic-block>\r\n    </div>\r\n    <div class=\"trees\">\r\n        <cutting-block\r\n            v-for=\"tree in currentTreeDataSource()\"\r\n            :key=\"tree.name\"\r\n 