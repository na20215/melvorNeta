{"ast":null,"code":"import basicInfoBlock from '../basicInfoBlock.vue';\nimport { fishingGrounds } from '../../../data/skillFishing';\nimport { useSkillStore } from '../../../stores/expPark';\nexport default {\n  components: {\n    basicInfoBlock\n  },\n  props: {\n    selectedFish: {\n      required: true,\n      default: ''\n    },\n    fishGround: {\n      required: true\n    }\n  },\n  setup(props) {\n    const SkillStore = useSkillStore();\n    const getMasteryExp = SkillStore.getMasteryExp;\n    const maxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel));\n    const lastMaxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel - 1));\n    return {\n      getMasteryExp,\n      maxExp,\n      lastMaxExp\n    };\n  },\n  data() {\n    return {\n      isActived: false\n    };\n  },\n  methods: {\n    updateFishName() {\n      this.isActived = true;\n      this.$emit('updateFishName', {\n        fishName: this.selectedFish.name\n      });\n    },\n    stopWorking() {\n      this.isActived = false;\n      this.$emit('stopWorking');\n    }\n  },\n  computed: {\n    theFish() {\n      return fishingGrounds[this.fishGround].fishInGround[this.selectedFish.name];\n    }\n  },\n  watch: {\n    selectedFish() {\n      this.isActived = false;\n      const averageTime = (this.theFish.maxTime + this.theFish.minTime) / 2;\n      this.theFish.addMasteryExp = Math.round(this.getMasteryExp(this.theFish.masteryLevel, 23, averageTime));\n      this.theFish.addMasteryPoolExp = Math.ceil(this.theFish.addMasteryExp / 4);\n    }\n  }\n};","map":{"version":3,"names":["basicInfoBlock","fishingGrounds","useSkillStore","components","props","selectedFish","required","default","fishGround","setup","SkillStore","getMasteryExp","maxExp","computed","getMaxExp","masteryLevel","lastMaxExp","data","isActived","methods","updateFishName","$emit","fishName","name","stopWorking","theFish","fishInGround","watch","averageTime","maxTime","minTime","addMasteryExp","Math","round","addMasteryPoolExp","ceil"],"sources":["C:\\Users\\10433\\Downloads\\vues\\vue-cli-01-a-new-vue-project\\src\\components\\layout\\skills\\fishingScreen.vue"],"sourcesContent":["<template>\r\n\r\n  <span v-if= \"selectedFish ===''\">这里将会显示这个鱼池里要钓的鱼的信息</span>\r\n  <div v-else class=\"screen\">\r\n    <img class=\"logo\" :src=\"selectedFish.icon\">\r\n    <span>{{ selectedFish.name }}</span> \r\n    <div class=\"expPart\">\r\n        <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/xp.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.exp}}\r\n                </template>\r\n        </basic-info-block>\r\n        <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/mastery_header.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.addMasteryExp}}\r\n                </template>\r\n        </basic-info-block>\r\n        <basic-info-block>\r\n            <img class=\"action\" src=\"../../../assets/main/mastery_pool.svg\">\r\n                <template v-slot:count>\r\n                    {{selectedFish.addMasteryPoolExp}}\r\n                </template>\r\n        </basic-info-block>\r\n    </div>\r\n    <p style=\"font-size: 0.85rem;\">钓鱼时间：{{ selectedFish.minTime }} - {{ selectedFish.maxTime }}秒</p>\r\n    <div class=\"masteryPart\">\r\n        <div class=\"iconPart\">\r\n            <img class=\"masteryIcon\" src=\"../../../assets/main/mastery_header.svg\">\r\n            <div class=\"masteryLevel\">{{ selectedFish.masteryLevel }}</div>\r\n        </div>\r\n        <div class=\"processBarPart\">\r\n            <div class=\"textPart\">{{ selectedFish.masteryExp }}/</div>\r\n            <div class=\"bar\"></div>\r\n        </div>\r\n    </div>\r\n    <button v-if=\"!isActived\" @click=\"updateFishName\">开始钓鱼</button>\r\n    <button v-else @click=\"stopWorking\">结束钓鱼</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport basicInfoBlock from '../basicInfoBlock.vue';\r\nimport { fishingGrounds } from '../../../data/skillFishing';\r\nimport { useSkillStore } from '../../../stores/expPark';\r\nexport default {\r\n    components:{basicInfoBlock},\r\n\r\n    props:{\r\n        selectedFish:{\r\n            required:true,\r\n            default:'',\r\n        },\r\n        fishGround:{\r\n            required:true,\r\n\r\n        }\r\n    },\r\n    setup(props) {\r\n        const SkillStore = useSkillStore();\r\n        const getMasteryExp = SkillStore.getMasteryExp;\r\n        const maxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel));\r\n        const lastMaxExp = computed(() => SkillStore.getMaxExp(props.selectedFish.masteryLevel - 1));\r\n        return{\r\n            getMasteryExp,\r\n            maxExp,\r\n            lastMaxExp\r\n        }\r\n    },\r\n    data(){\r\n        return{\r\n            isActived:false,\r\n        };\r\n        \r\n    },\r\n    methods:{\r\n        updateFishName(){\r\n            this.isActived = true;\r\n            this.$emit('updateFishName', {fishName: this.selectedFish.name});\r\n        },\r\n        stopWorking(){\r\n            this.isActived = false;\r\n            this.$emit('stopWorking')\r\n        }\r\n    },\r\n    computed:{\r\n        theFish(){\r\n            return  fishingGrounds[this.fishGround].fishInGround[this.selectedFish.name]\r\n        }\r\n    },\r\n    watch:{\r\n        selectedFish(){\r\n            this.isActived = false;\r\n            const averageTime = (this.theFish.maxTime + this.theFish.minTime)/2;\r\n            this.theFish.addMasteryExp = Math.round(this.getMasteryExp(this.theFish.masteryLevel,23,averageTime));\r\n            this.theFish.addMasteryPoolExp = Math.ceil(this.theFish.addMasteryExp/4);\r\n        }\r\n\r\n    }\r\n\r\n    \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .screen{\r\n        position: relative;\r\n        display: flex;\r\n        width: 100%;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    .expPart{\r\n        display: flex;\r\n        flex-direction: row;\r\n        gap: 1rem;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        box-sizing: border-box;\r\n        padding: 0.5rem 1rem;\r\n        width: 50%;\r\n        \r\n    }\r\n    .masteryPart{\r\n        display: flex;\r\n        flex-direction: row;\r\n        width: 100%;\r\n        height: 2rem;\r\n        padding: 0.5rem 0;\r\n        box-sizing: border-box;\r\n    }\r\n    .iconPart{\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 100%;\r\n        width: 10%;\r\n    }\r\n    .masteryIcon{\r\n        height: 1rem;\r\n        width: 1rem;\r\n    }\r\n    .masteryLevel{\r\n        font-size: 1rem;\r\n        font-weight: 700;\r\n    }\r\n    .processBarPart{\r\n        display: flex;\r\n        flex-direction: column;\r\n        width: 90%;\r\n        align-items: center;\r\n    }\r\n    .textPart{\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        font-size: 0.8rem;\r\n    }\r\n    .bar{\r\n        width: 100%;\r\n    }\r\n    .action{\r\n        height: 1.5rem;\r\n        width: 1.5rem;\r\n    }\r\n    .logo{\r\n        position: absolute;\r\n        left: 1rem;\r\n        top: 1rem;\r\n        height: 2rem;\r\n        width: 2rem;\r\n    }\r\n</style>"],"mappings":"AA2CA,OAAOA,cAAa,MAAO,uBAAuB;AAClD,SAASC,cAAa,QAAS,4BAA4B;AAC3D,SAASC,aAAY,QAAS,yBAAyB;AACvD,eAAe;EACXC,UAAU,EAAC;IAACH;EAAc,CAAC;EAE3BI,KAAK,EAAC;IACFC,YAAY,EAAC;MACTC,QAAQ,EAAC,IAAI;MACbC,OAAO,EAAC;IACZ,CAAC;IACDC,UAAU,EAAC;MACPF,QAAQ,EAAC;IAEb;EACJ,CAAC;EACDG,KAAKA,CAACL,KAAK,EAAE;IACT,MAAMM,UAAS,GAAIR,aAAa,CAAC,CAAC;IAClC,MAAMS,aAAY,GAAID,UAAU,CAACC,aAAa;IAC9C,MAAMC,MAAK,GAAIC,QAAQ,CAAC,MAAMH,UAAU,CAACI,SAAS,CAACV,KAAK,CAACC,YAAY,CAAC